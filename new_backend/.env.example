# Database (Docker Postgres)
DATABASE_URL=postgresql://voiceflow_user:voiceflow_pass@postgres:5432/voiceflow

# Groq Cloud
GROQ_API_KEY=your_groq_api_key_here

# Chroma (Docker service)
CHROMA_URL=http://chroma:8000
CHROMA_PATH=/chroma/chroma

# Redis (Docker service)
REDIS_HOST=redis
REDIS_PORT=6379

# MinIO (Docker S3)
MINIO_ENDPOINT=http://minio:9000
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin

# Twilio
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number

# FastAPI (Docker service)
FASTAPI_URL=http://ingestion-service:8001

# Ingestion settings
EMBEDDING_MODEL=all-MiniLM-L6-v2
ASR_ENGINE=vosk
TTS_ENGINE=coqui
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
STORE_RAW_HTML=false
PII_PROTECTION=false

# API Keys
INTERNAL_API_KEY=dev-internal-key-please-change